[user]
	email = tim.blakely@gmail.com
	name = Tim Blakely
[alias]
	amend = commit --amend --no-edit
	delete-merged-branches = "!git checkout master && git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
[diff]
	tool = meld
[difftool]
	prompt = false
[difftool "meld"]
	cmd = meld "$LOCAL" "$REMOTE"
